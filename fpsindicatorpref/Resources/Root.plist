<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Main Toggle -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Status</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>enabled</string>
			<key>label</key>
			<string>Enable FPS Indicator</string>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
			<key>requiresRespring</key>
			<true/>
		</dict>
		
		<!-- App Selection -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Applications</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
				<string>Select Apps</string>
			<key>action</key>
			<string>selectApps</string>
		</dict>
		
		 <!-- PUBG Mobile Settings -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>PUBG Mobile Settings</string>
			<key>footerText</key>
			<string>These settings help avoid anti-cheat detection in PUBG Mobile games. Changing these settings requires a restart of PUBG Mobile to take effect.

When using Log File mode, FPS data is saved to /var/mobile/Documents/FPSIndicator/Logs/ to avoid detection by anti-cheat systems.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>usePUBGSpecialMode</string>
			<key>label</key>
			<string>Use PUBG Special Mode</string>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
			<key>requiresRestart</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
				<string>PSSegmentCell</string>
			<key>default</key>
			<integer>1</integer>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>pubgStealthMode</string>
			<key>label</key>
			<string>Stealth Level</string>
			<key>validTitles</key>
			<array>
				<string>Normal</string>
				<string>Medium</string>
				<string>Maximum</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
			</array>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
			<key>requiresRestart</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSegmentCell</string>
			<key>default</key>
			<integer>0</integer>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>pubgUiMode</string>
			<key>label</key>
			<string>UI Integration Mode</string>
			<key>validTitles</key>
			<array>
				<string>Standard</string>
				<string>Fire Button</string>
				<string>Log File</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
			</array>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
			<key>requiresRestart</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>default</key>
			<real>2.0</real>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>pubgRefreshRate</string>
			<key>min</key>
			<real>1.0</real>
			<key>max</key>
			<real>10.0</real>
			<key>showValue</key>
			<true/>
			<key>label</key>
			<string>Refresh Rate (Hz)</string>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>useQuartzCoreAPI</string>
			<key>label</key>
			<string>Use QuartzCore Debug API</string>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>useCoreAnimationPerfHUD</string>
			<key>label</key>
			<string>Use CA Performance HUD</string>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
		</dict>
		
		<!-- Simple Appearance -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Appearance</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSegmentCell</string>
			<key>default</key>
			<integer>0</integer>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>position</string>
			<key>label</key>
			<string>Position</string>
			<key>validTitles</key>
			<array>
				<string>Top Right</string>
				<string>Top Left</string>
				<string>Bottom Right</string>
				<string>Bottom Left</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
				<integer>2</integer>
				<integer>3</integer>
			</array>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>default</key>
			<string>#00FF00</string>
			<key>defaults</key>
			<string>com.fpsindicator</string>
			<key>key</key>
			<string>color</string>
			<key>label</key>
			<string>Text Color</string>
			<key>validTitles</key>
			<array>
				<string>Green</string>
				<string>Yellow</string>
				<string>Red</string>
				<string>White</string>
				<string>Blue</string>
			</array>
			<key>validValues</key>
			<array>
				<string>#00FF00</string>
				<string>#FFFF00</string>
				<string>#FF0000</string>
				<string>#FFFFFF</string>
				<string>#0000FF</string>
			</array>
			<key>PostNotification</key>
			<string>com.fpsindicator/loadPref</string>
		</dict>
		
		<!-- Log File Management -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Log Files</string>
			<key>footerText</key>
			<string>View and share FPS log files created when using the Log File UI integration mode in PUBG Mobile.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>View Log Files</string>
			<key>action</key>
			<string>viewLogFiles</string>
		</dict>
		
		<!-- Footer Section -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>FPSIndicator shows frames per second in compatible apps and games.
			
Restart the app after changing settings.</string>
		</dict>
	</array>
	<key>title</key>
	<string>FPS Indicator</string>
</dict>
</plist>
